{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.ts",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/.+",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
